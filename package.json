{
  "name": "mobx-history",
  "version": "0.1.6",
  "description": "Mobx History",
  "main": "index.js",
  "repository": "zjuasmn/mobx-history",
  "scripts": {
    "test": "mocha --require babel-register src/__test__",
    "build-lib": "babel src -d . --ignore __test__",
    "build-umd": "webpack src/index.js umd/mobx-history.js",
    "build-min": "webpack -p src/index.js umd/mobx-history.min.js",
    "build": "node ./scripts/build.js",
    "prerelease":"npm run build",
    "release": "node ./scripts/release.js"
  },
  "keywords": [
    "mobx",
    "history"
  ],
  "author": "zjuasmn <zjuasmn@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "debug": "^2.6.1",
    "history": "^4.5.1",
    "mobx": "^3.1.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "jasmine": "^2.5.3",
    "mocha": "^3.2.0",
    "readline-sync": "^1.4.6"
  }
}
